public class Dechargeur implements Runnable{

    
    private AleaStock stock;
    private Chariot   chariot;
    private Boolean   fin=true;

    public Dechargeur(AleaStock stock, Chariot chariot){
	this.stock   = stock;
	this.chariot = chariot;
    }

    public void run(){
	while(fin){
	    chariot.decharger();
	    
	}
    }
}
